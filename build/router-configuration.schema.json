{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "definitions": {
        "Cookie": {
            "properties": {
                "options": {
                    "$ref": "#/definitions/CookieOptions"
                },
                "value": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "CookieMap": {
            "additionalProperties": {
                "$ref": "#/definitions/Cookie"
            },
            "type": "object"
        },
        "CookieOptions": {
            "properties": {
                "domain": {
                    "type": "string"
                },
                "encode": {
                    "type": "object"
                },
                "expires": {
                    "anyOf": [
                        {
                            "description": "Enables basic storage and retrieval of dates and times.",
                            "format": "date-time",
                            "type": "string"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                },
                "httpOnly": {
                    "type": "boolean"
                },
                "maxAge": {
                    "type": "number"
                },
                "path": {
                    "type": "string"
                },
                "sameSite": {
                    "type": [
                        "string",
                        "boolean"
                    ]
                },
                "secure": {
                    "enum": [
                        "auto",
                        false,
                        true
                    ]
                },
                "signed": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "NamedPattern": {
            "additionalProperties": {
                "type": "string"
            },
            "type": "object"
        },
        "RouteConfiguration": {
            "properties": {
                "acceptedVerbs": {
                    "items": {
                        "enum": [
                            "CONNECT",
                            "DELETE",
                            "GET",
                            "HEAD",
                            "OPTIONS",
                            "POST",
                            "PUT"
                        ],
                        "type": "string"
                    },
                    "type": "array"
                },
                "debug": {
                    "type": "boolean"
                },
                "defaultParams": {
                },
                "defaultStatusCode": {
                    "type": "number"
                },
                "errorRoute": {
                    "type": "string"
                },
                "metaData": {
                },
                "name": {
                    "type": "string"
                },
                "pattern": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/NamedPattern"
                        },
                        {
                            "type": "string"
                        }
                    ]
                },
                "queryDelimiter": {
                    "type": "string"
                },
                "queryEquals": {
                    "type": "string"
                },
                "tasks": {
                    "items": {
                        "anyOf": [
                            {
                                "$ref": "#/definitions/RouteTaskConfiguration<any>"
                            },
                            {
                                "type": "string"
                            }
                        ]
                    },
                    "type": "array"
                }
            },
            "type": "object"
        },
        "RouteTaskConfiguration<any>": {
            "properties": {
                "config": {
                },
                "errorRoute": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "renderer": {
                    "type": "string"
                },
                "taskModule": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "RouterResponse": {
            "properties": {
                "body": {
                    "type": "string"
                },
                "clearCookies": {
                    "$ref": "#/definitions/CookieMap"
                },
                "contentType": {
                    "type": "string"
                },
                "cookies": {
                    "$ref": "#/definitions/CookieMap"
                },
                "headers": {
                    "additionalProperties": {
                        "type": "string"
                    },
                    "type": "object"
                },
                "statusCode": {
                    "type": "number"
                }
            },
            "type": "object"
        }
    },
    "properties": {
        "disasterResponse": {
            "$ref": "#/definitions/RouterResponse"
        },
        "domains": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "metaData": {
        },
        "name": {
            "type": "string"
        },
        "routes": {
            "items": {
                "$ref": "#/definitions/RouteConfiguration"
            },
            "type": "array"
        }
    },
    "type": "object"
}

